@model AthleteModel

<h4>Enter Athlete</h4>


@using (Html.BeginForm("Create", "Athlete", FormMethod.Post))
{
    <div class="container-full" style="background-color: white; padding: 20px">
        <div class="container">
            @Html.LabelFor(x => x.Name)
            @Html.TextBoxFor(x => x.Name)
            @Html.LabelFor(x => x.Surname)
            @Html.TextBoxFor(x => x.Surname)
            @Html.LabelFor(x => x.Country)
            @Html.TextBoxFor(x => x.Country)
            <input type="submit" value="Submit" />
        </div>
    </div>
}
