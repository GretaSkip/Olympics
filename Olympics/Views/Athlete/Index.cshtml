@model List<AthleteModel>

<h3>Athletes:</h3>

<table class="table">
    <tr>
        <th>ID</th>
        <th>Name and Surname</th>
        <th>Country</th>
    </tr>
    @foreach (var item in @Model)
    {
<tr>
    <td>@item.Id </td>
    <td>@item.Name @item.Surname </td>
    <td>@item.Country </td>
    
</tr>
    }
</table>
